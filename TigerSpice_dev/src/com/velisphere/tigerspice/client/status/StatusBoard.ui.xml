<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:vssb="urn:import:com.velisphere.tigerspice.client.sidebars">
	<g:HTMLPanel>

		<b:Breadcrumbs divider=">" ui:field="brdMain">
		</b:Breadcrumbs>

		<b:Row>

		    
		    <b:Column size="12" offset="0">
				<b:PageHeader ui:field="pageHeader">Status</b:PageHeader>
			</b:Column>
		</b:Row>
			

	
				

		<b:Row>
		
		<b:Column size="10" offset="0">
		

		
		<b:TabPanel tabPosition="above" ui:field="tabPanel">
		
			<b:TabPane heading="News" active="true">
			    
				<g:HTML><b>News</b><br></br><br></br></g:HTML>
				<g:HTML>VeliSphere is now running in a public evaluation phase. Feel free to test out the functionality and 
				features, but bear in min that we provide this service to you with no warranty or performance guarantee.<br></br><br></br>
				<b>Evaluation is explicitly at your own risk</b></g:HTML>
				
					
			
			</b:TabPane>
		
		
		    <b:TabPane heading="Subscription" active="false">
		    <b:Row>
		<b:Column size="6" offset="0">
			<g:HTML><b>Subscription Information</b><br></br><br></br></g:HTML>
			
			</b:Column>	
		</b:Row>
		
		
		
		<b:Row>
			<b:Column size="8" offset="0">
				<b:StackProgressBar ui:field="pgbUtilization">
					<b:Bar color="SUCCESS" ui:field="pgbGreen" text="Utilization" percent="0" />
					<b:Bar color="WARNING" ui:field="pgbYellow" text="" percent="0" />
					<b:Bar color="DANGER" ui:field="pgbRed" text="" percent="0" />
				</b:StackProgressBar>
			</b:Column>
			
		</b:Row>
		
		<b:Row>
		<b:Column size="8" offset="0">
			 <b:Paragraph ui:field="pgpUtilInfo" addStyleNames="smalltext">Upgrade your subscription to increase your endpoint limit.</b:Paragraph>
			  <b:Paragraph addStyleNames="smalltext">&nbsp;</b:Paragraph>
			</b:Column>
		</b:Row>
		
		    </b:TabPane>
		    
		    
		    	<b:TabPane heading="Service Status" active="false">
			
			<g:HTML><b>Service Status</b><br></br><br></br>
							No service disruptions have been repored.</g:HTML>
						
			
			</b:TabPane>
			<b:TabPane heading="Alerts" active="false" ui:field="tbpAlerts">
			    
				 <b:Paragraph addStyleNames="smalltext">&nbsp;</b:Paragraph>
			</b:TabPane>
			
		
		</b:TabPanel>
			    
		
		
	</b:Column>

<b:Column size="2" offset="0">
		
    <vssb:General></vssb:General>
			
			</b:Column>	
		
</b:Row>

	</g:HTMLPanel>
</ui:UiBinder> 
